<!-- Create your form here -->
<h1>Student Form</h1>
<%= form_tag students_path, method: :post do %>
  <%= label_tag :first_name %>
  <%= text_field_tag :'student[first_name]' %><br><br>
  <%= label_tag :last_name %>
  <%= text_field_tag :'student[last_name]' %><br><br>
  <%= submit_tag "Create" %>
<% end %>
